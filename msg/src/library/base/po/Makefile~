po2html:
	cat ko.po > tmp.txt
	sed -e '/^[#"]/d' tmp.txt > tmp2.txt
	sed -e 's/msgid/[English]/g' tmp2.txt > en.txt
	sed -e 's/msgstr/[한국어]/g' en.txt > ko.txt
	dwww-txt2html --utf8 ko.txt > ko.html
	rm *.txt
	cat R-ko.po > tmp.txt
	sed -e '/^[#"]/d' tmp.txt > tmp2.txt
	sed -e 's/msgid/[English]/g' tmp2.txt > R-en.txt
	sed -e 's/msgstr/[한국어]/g' R-en.txt > R-ko.txt
	dwww-txt2html --utf8 R-ko.txt > R-ko.html
	rm *.txt

