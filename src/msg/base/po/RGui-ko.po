#: src/gnuwin32/console.c:2232
msgid "[35mScrolling.\n[39;49m"
msgstr "[34m스크롤링.\n[39;49m"

#: src/gnuwin32/console.c:2233
msgid "[35m  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n[39;49m"
msgstr "[34m  키보드: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n[39;49m"

#: src/gnuwin32/console.c:2234
msgid ""
"[35m  Mouse: use the scrollbar(s).\n[39;49m"
"[35m\n[39;49m"
msgstr ""
"[34m  마우스: 스크롤바를 사용하세요.\n[39;49m"
"[34m\n[39;49m"

#: src/gnuwin32/console.c:2235
msgid "[35mEditing.\n[39;49m"
msgstr "[34m편집.\n[39;49m"

#: src/gnuwin32/console.c:2236
msgid "[35m  Moving the cursor: \n[39;49m"
msgstr "[34m  커서를 이동중: \n[39;49m"

#: src/gnuwin32/console.c:2237
msgid "[35m     Left arrow or Ctrl+B: move backward one character;\n[39;49m"
msgstr "[34m     좌로 이동화살표 또는 Ctrl+B: 한 문자 뒤로 이동합니다;\n[39;49m"

#: src/gnuwin32/console.c:2238
msgid "[35m     Right arrow or Ctrl+F: move forward one character;\n[39;49m"
msgstr "[34m     우로 이동화살표 또는 Ctrl+F: 한 문자 앞으로 이동합니다;\n[39;49m"

#: src/gnuwin32/console.c:2239
msgid "[35m     Home or Ctrl+A: go to beginning of line;\n[39;49m"
msgstr "[34m     Home 또는 Ctrl+A: 라인의 맨 처음으로 이동합니다;\n[39;49m"

#: src/gnuwin32/console.c:2240
msgid "[35m     End or Ctrl+E: go to end of line;\n[39;49m"
msgstr "[34m     End 또는 Ctrl+E: 라인의 맨 끝으로 이동합니다;\n[39;49m"

#: src/gnuwin32/console.c:2241
msgid "[35m  History: Up and Down Arrows, Ctrl+P, Ctrl+N\n[39;49m"
msgstr "[34m  작업기록: 위로 이동과 아래로 이동 화살표들, Ctrl+P, Ctrl+N\n[39;49m"

#: src/gnuwin32/console.c:2242
msgid "[35m  Deleting:\n[39;49m"
msgstr "[34m  삭제:\n[39;49m"

#: src/gnuwin32/console.c:2243
msgid "[35m     Del or Ctrl+D: delete current character or selection;\n[39;49m"
msgstr "[34m     Del 또는 Ctrl+D: 현재 문자 또는 선택 영역을 삭제합니다;\n[39;49m"

#: src/gnuwin32/console.c:2244
msgid "[35m     Backspace: delete preceding character;\n[39;49m"
msgstr "[34m     Backspace: 바로 이전문자를 삭제합니다;\n[39;49m"

#: src/gnuwin32/console.c:2245
msgid "[35m     Ctrl+Del or Ctrl+K: delete text from current character to end of line.\n[39;49m"
msgstr "[34m     Ctrl+Del 또는 Ctrl+K: 현재 문자부터 라인의 끝까지 삭제합니다.\n[39;49m"

#: src/gnuwin32/console.c:2246
msgid "[35m     Ctrl+U: delete all text from current line.\n[39;49m"
msgstr "[34m     Ctrl+U: 현재 라인의 모든 텍스트를 삭제합니다.\n[39;49m"

#: src/gnuwin32/console.c:2247
msgid "[35m  Copy and paste.\n[39;49m"
msgstr "[34m  복사와 붙여넣기.\n[39;49m"

#: src/gnuwin32/console.c:2248
msgid "[35m     Use the mouse (with the left button held down) to mark (select) text.\n[39;49m"
msgstr "[34m     텍스트를 표시 혹은 선택하려면 (좌측버튼을 누른채로) 마우스를 이용하세요.\n[39;49m"

#: src/gnuwin32/console.c:2249
msgid "[35m     Use Shift+Del (or Ctrl+C) to copy the marked text to the clipboard and\n[39;49m"
msgstr "[34m     선택한 텍스트를 클립보드에 복사하기 위해서는 Shift+Del (또는 Ctrl+C)을 사용하세요\n[39;49m"

#: src/gnuwin32/console.c:2250
msgid "[35m     Shift+Ins (or Ctrl+V or Ctrl+Y) to paste the content of the clipboard (if any)  \n[39;49m"
msgstr "[34m     (만약) 클립보드에 있는 내용을 붙여넣고자 한다면 Shift+Ins (또는 Ctrl+V 혹은 Ctrl+Y)을 이용하세요  \n[39;49m"

#: src/gnuwin32/console.c:2251
msgid "[35m     to the console, Ctrl+X first copy then paste\n[39;49m"
msgstr ""

#: src/gnuwin32/console.c:2252
msgid "[35m  Misc:\n[39;49m"
msgstr "[34m  기타:\n[39;49m"

#: src/gnuwin32/console.c:2253
msgid "[35m     Ctrl+L: Clear the console.\n[39;49m"
msgstr "[34m     Ctrl+L: 콘솔을 지워 깨끗이 합니다.\n[39;49m"

#: src/gnuwin32/console.c:2254
msgid "[35m     Ctrl+O or INS: Toggle overwrite mode: initially off.\n[39;49m"
msgstr "[34m     Ctrl+O 또는 INS: 덮어쓰기 모드로 전환합니다: 처음에는 꺼져있습니다.\n[39;49m"

#: src/gnuwin32/console.c:2255
msgid "[35m     Ctrl+T: Interchange current char with one to the left.\n[39;49m"
msgstr "[34m     Ctrl+T: 현재의 문자와 좌측에 있는 문자를 서로 교환합니다.\n[39;49m"

#: src/gnuwin32/console.c:2256
msgid ""
"[35m\n[39;49m"
"[35mNote: Console is updated only when some input is required.\n[39;49m"
msgstr ""
"[34m\n[39;49m"
"[34m참고: 콘솔은 오로지 어떤 입력이 요구되어 질 때만 갱신됩니다.\n[39;49m"

#: src/gnuwin32/console.c:2257
msgid ""
"[35m  Use Ctrl+W to toggle this feature off/on.\n[39;49m"
"[35m\n[39;49m"
msgstr ""
"[34m  이 기능을 켜거나 혹은 끄고 싶을 때 Ctrl+W 를 이용하세요.\n[39;49m"
"[34m\n[39;49m"

#: src/gnuwin32/console.c:2258
msgid ""
"[35mUse ESC to stop the interpreter.\n[39;49m"
"[35m\n[39;49m"
msgstr ""
"[34m인터프리터를 중단하기 위해서는 ESC 를 이용하세요.\n[39;49m"
"[34m\n[39;49m"

#: src/gnuwin32/console.c:2259
msgid ""
"[35mTAB starts completion of the current word.\n[39;49m"
"[35m\n[39;49m"
msgstr ""

#: src/gnuwin32/console.c:2260
msgid "[35mStandard Windows hotkeys can be used to switch to the\n[39;49m"
msgstr ""

#: src/gnuwin32/console.c:2261
msgid "[35mgraphics device (Ctrl+Tab or Ctrl+F6 in MDI, Alt+Tab in SDI)[39;49m"
msgstr ""

