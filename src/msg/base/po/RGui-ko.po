#: src/gnuwin32/editor.c:608
msgid "[35mRUNNING COMMANDS\n[39;49m"
msgstr "[34m명령들을 실행중입니다\n[39;49m"

#: src/gnuwin32/editor.c:609
msgid "[35mTo run a line or section of R code, select the code and either\n[39;49m"
msgstr "[34mR 코드 한 줄 또는 선택한 영역을 실행하고자 한다면, 코드를 먼저 선택한 뒤 \n[39;49m"

#: src/gnuwin32/editor.c:610
