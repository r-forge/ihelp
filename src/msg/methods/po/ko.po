#: class_support.c:32
msgid "[35mcalling the C routine used as an initializer for 'externalptr' objects[39;49m"
msgstr "[34m'externalptr' 객체들에 대한 초기화 매체로서 사용될 C 루틴을 호출합니다[39;49m"

#: do_substitute_direct.c:41
msgid "[35minvalid list for substitution[39;49m"
msgstr "[34m대입에 유효하지 않은 목록입니다[39;49m"

#: methods_list_dispatch.c:149
msgid "[35mcould not find the skeleton calls for 'methods' (package detached?): expect very bad things to happen[39;49m"
msgstr "[34m'methods'에 대한 skeleton calls를 찾지 못했습니다 (패키지가 분리되어 있나요?):  매우 안 좋은 경우가 발생할 것 같습니다[39;49m"

#: methods_list_dispatch.c:215
#, [4mc-format[24m
msgid "[35mno \"allMethods\" slot found in object of class \"[1m%s(B[m[35m\" used as methods list for function '[1m%s(B[m[35m'[39;49m"
msgstr "[34m함수 '[1m%2$s(B[m[34m'에 대한 메소드 목록으로서 사용될 클래스 \"[1m%1$s(B[m[34m\"의 객체 내에서 \"allMethods\" 슬롯을 찾을 수 없습니다[39;49m"

#: methods_list_dispatch.c:443
#, [4mc-format[24m
msgid "[35mno generic function definition found for '[1m%s(B[m[35m'[39;49m"
msgstr "[34m'[1m%s(B[m[34m'을 위한 제네릭 함수정의를 찾을 수 없습니다[39;49m"

#: methods_list_dispatch.c:446
#, [4mc-format[24m
msgid "[35mno generic function definition found for '[1m%s(B[m[35m' in the supplied environment[39;49m"
msgstr "[34m주어진 environment에서 '[1m%s(B[m[34m'를 위한 제네릭 함수정의를 찾을 수 없습니다[39;49m"

#: methods_list_dispatch.c:481
#, [4mc-format[24m
msgid "[35minvalid generic function object for method selection for function '[1m%s(B[m[35m': expected a function or a primitive, got an object of class \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m함수 '[1m%1$s(B[m[34m'를 위한 메소드선택에 유효하지 않은 제네릭 함수객체입니다:  함수 또는 프리미티브가 와야하는데 클래스 \"[1m%2$s(B[m[34m\"의 객체를 얻었습니다[39;49m"

#: methods_list_dispatch.c:496
#, [4mc-format[24m
msgid "[35mno direct or inherited method for function '[1m%s(B[m[35m' for this call[39;49m"
msgstr "[34m이 호출을 위해 함수 '[1m%s(B[m[34m'에 대한 직접적 또는 상속적인 메소드가 없습니다[39;49m"

#: methods_list_dispatch.c:522 methods_list_dispatch.c:1042
msgid "[35minvalid object (non-function) used as method[39;49m"
msgstr "[34m메소드로서 사용된 유효하지 않은 객체입니다[39;49m"

#: methods_list_dispatch.c:543
#, [4mc-format[24m
msgid "[35mcould not find symbol '[1m%s(B[m[35m' in frame of call[39;49m"
msgstr "[34m호출 프레임에서 심볼 '[1m%s(B[m[34m'를 찾을 수 없었습니다[39;49m"

#: methods_list_dispatch.c:551
#, [4mc-format[24m
msgid "[35minvalid symbol in checking for missing argument in method dispatch: expected a name, got an object of class \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m메소드 디스패치에서 누락된 인자를 확인하는데 유효하지 않은 심볼입니다:  이름이 와야하는데 클래스 \"[1m%s(B[m[34m\"의 객체를 얻었습니다[39;49m"

#: methods_list_dispatch.c:554
msgid "[35muse of NULL environment is defunct[39;49m"
msgstr "[34mNULL environment의 사용은 더이상 지원되지 않습니다[39;49m"

#: methods_list_dispatch.c:558
#, [4mc-format[24m
msgid "[35minvalid environment in checking for missing argument, '[1m%s(B[m[35m', in methods dispatch: got an object of class \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m메소드 디스패치에 누락된 인자 '[1m%1$s(B[m[34m'를 확인하는데 유효하지 않은 environment인데, 클래스 \"[1m%2$s(B[m[34m\"의 객체를 얻었습니다[39;49m"

#: methods_list_dispatch.c:584
#, [4mc-format[24m
msgid "[35mobject of class \"[1m%s(B[m[35m\" used as methods list for function '[1m%s(B[m[35m' ( no 'argument' slot)[39;49m"
msgstr "[34m함수 '[1m%2$s(B[m[34m'를 위한 메소드 목록으로서 사용된 클래스 \"[1m%1$s(B[m[34m\"의 객체입니다 ('argument' 슬롯이 없습니다)[39;49m"

#: methods_list_dispatch.c:595
#, [4mc-format[24m
msgid "[35m(in selecting a method for function '[1m%s(B[m[35m') '...' and related variables cannot be used for methods dispatch[39;49m"
msgstr "[34m(함수 '[1m%s(B[m[34m'를 위한 메소드를 선택을 할 때) '...'와 관계가 있는 변수들은 메소드 디스패치에 사용될 수 없습니다[39;49m"

#: methods_list_dispatch.c:598
#, [4mc-format[24m
msgid "[35m(in selecting a method for function '[1m%s(B[m[35m') the 'environment' argument for dispatch must be an R environment; got an object of class \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m(함수 '[1m%1$s(B[m[34m'를 위한 메소드를 선택할 때) 디스패치를 위한 'environment' 인자는 반드시 R environment이어야 하는데, 클래스 \"[1m%2$s(B[m[34m\"를 얻습니다[39;49m"

#: methods_list_dispatch.c:612 methods_list_dispatch.c:624
#: methods_list_dispatch.c:995
#, [4mc-format[24m
msgid "[35merror in evaluating the argument '[1m%s(B[m[35m' in selecting a method for function '[1m%s(B[m[35m': [1m%s[39;49m(B[m"
msgstr "[34m함수 '[1m%2$s(B[m[34m'를 위한 메소드 선택시 인수 '[1m%1$s(B[m[34m'를 평가하는데 오류가 발생했습니다: [1m%3$s[39;49m(B[m"

#: methods_list_dispatch.c:632
#, [4mc-format[24m
msgid "[35mno matching method for function '[1m%s(B[m[35m' (argument '[1m%s(B[m[35m', with class \"[1m%s(B[m[35m\")[39;49m"
msgstr "[34m함수 '[1m%1$s(B[m[34m'에 일치하는 메소드가 없습니다 (클래스 \"[1m%3$s(B[m[34m\"을 가지는 인자 '[1m%2$s(B[m[34m')[39;49m"

#: methods_list_dispatch.c:639
#, [4mc-format[24m
msgid "[35mrecursive use of function '[1m%s(B[m[35m' in method selection, with no default method[39;49m"
msgstr "[34m메소드 선택시 기본 메소드가 없는 함수 '[1m%s(B[m[34m'의 재귀적 사용입니다[39;49m"

#: methods_list_dispatch.c:699
#, [4mc-format[24m
msgid "[35merror in evaluating a 'primitive' next method: [1m%s[39;49m(B[m"
msgstr "[34m'primitive' 다음 메소드를 평가하는 중에 에러가 발생했습니다:[1m%s[39;49m(B[m"

#: methods_list_dispatch.c:797
#, [4mc-format[24m
msgid "[35m'[1m%s(B[m[35m' must be a single string (got a character vector of length [1m%d(B[m[35m)[39;49m"
msgstr "[34m'[1m%s(B[m[34m'는 반드시 단일 문자열이어야 합니다 (길이가 [1m%d(B[m[34m인 문자형 벡터를 얻습니다)[39;49m"

#: methods_list_dispatch.c:801
#, [4mc-format[24m
msgid "[35m'[1m%s(B[m[35m' must be a non-empty string; got an empty string[39;49m"
msgstr "[34m'[1m%s(B[m[34m'는 반드시 빈 문자열이어야 하는데, 빈 문자열을 얻었습니다[39;49m"

#: methods_list_dispatch.c:805
#, [4mc-format[24m
msgid "[35m'[1m%s(B[m[35m' must be a single string (got an object of class \"[1m%s(B[m[35m\")[39;49m"
msgstr "[34m'[1m%s(B[m[34m'는 반드시 단일 문자열이어야 합니다 (클래스 \"[1m%s(B[m[34m\"의 객체를 얻었습니다)[39;49m"

#: methods_list_dispatch.c:876
msgid "[35mclass should be either a character-string name or a class definition[39;49m"
msgstr "[34m클래스는 문자열 이름 또는 클래스 정의 중 하나 이어야 합니다[39;49m"

#: methods_list_dispatch.c:955
#, [4mc-format[24m
msgid "[35mfailed to get the generic for the primitive \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m프리미티브 \"[1m%s(B[m[34m\"를 위한 제네릭을 얻는데 실패했습니다[39;49m"

#: methods_list_dispatch.c:961
#, [4mc-format[24m
msgid "[35mexpected a generic function or a primitive for dispatch, got an object of class \"[1m%s(B[m[35m\"[39;49m"
msgstr "[34m제네릭 함수 또는 디스패치를 위한 프리미티브가 와야 하는데, 클래스 \"[1m%s(B[m[34m\"의 객체를 얻었습니다[39;49m"

[32m#~ msgid "[35min processing 'callNextMethod', found a '...' in the matched call, but no corresponding '...' argument[32m"[39;49m
[32m#~ msgstr "[34m'callNextMethod'를 처리중, 일치하는 호출에서 '...'를 찾았지만, 상응하느느 '...' 인자는 없었습니다[32m"[39;49m
