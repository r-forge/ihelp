#: fork.c:253 fork.c:256
msgid "[35munable to create a pipe[39;49m"
msgstr "[34m파이프를 생성할 수 없습니다[39;49m"

#: fork.c:274
#, [4mc-format[24m
msgid "[35munable to fork, possible reason: [1m%s[39;49m(B[m"
msgstr "[34m포크할 수 없습니다. 가능한 이유는 다음과 같습니다: [1m%s[39;49m(B[m"

#: fork.c:320
msgid "[35mmemory allocation error[39;49m"
msgstr "[34m메모리 할당 에러입니다[39;49m"

#: fork.c:373
msgid "[35monly children can send data to the master process[39;49m"
msgstr "[34m오로지  자식들만이 데이터를 마스터 프로세스에 보낼 수 있습니다[39;49m"

#: fork.c:375
msgid "[35mthere is no pipe to the master process[39;49m"
msgstr "[34m마스터 프로세스로 연결된 파이프가 없습니다[39;49m"

#: fork.c:377
msgid "[35mcontent to send must be RAW, use serialize() if needed[39;49m"
msgstr "[34m보내야할 컨텐츠는 반드시 RAW이어야 하며, 필요하다면 serialize()를 이용하세요[39;49m"

#: fork.c:386 fork.c:393 fork.c:717
msgid "[35mwrite error, closing pipe to the master[39;49m"
msgstr "[34m쓰기 에러입니다.  마스에 연결된 파이프를 닫습니다[39;49m"

#: fork.c:406
msgid "[35monly the master process can send data to a child process[39;49m"
msgstr "[34m오로지 마스터 프로세스만이 데이터를 자식프로세스에 보낼 수 있습니다[39;49m"

#: fork.c:413
#, [4mc-format[24m
msgid "[35mchild [1m%d(B[m[35m does not exist[39;49m"
msgstr "[34mchild [1m%d(B[m[34m는 존재하지 않습니다[39;49m"

#: fork.c:419
msgid "[35mwrite error[39;49m"
msgstr "[34m쓰기 에러입니다[39;49m"

#: fork.c:487 fork.c:611
#, [4mc-format[24m
msgid "[35merror '[1m%s(B[m[35m' in select[39;49m"
msgstr "[34m선택중 에러 '[1m%s(B[m[34m'가 발생했습니다[39;49m"

#: fork.c:699
msgid "[35m'mckill' failed[39;49m"
msgstr "[34m'mckill' 실패했습니다[39;49m"

#: fork.c:709
msgid "[35m'mcexit' can only be used in a child process[39;49m"
msgstr "[34m'mcexit'는 오로지 자식 프로세스에서만 사용됩니다[39;49m"

#: fork.c:730
msgid "[35m'mcexit' failed[39;49m"
msgstr "[34m'mcexit' 실패했습니다[39;49m"

#: fork.c:759 fork.c:768
msgid "[35minvalid CPU affinity specification[39;49m"
msgstr "[34m유효하지 않은 CPU affinity 지정입니다[39;49m"

#: fork.c:779
msgid "[35mrequested CPU set is too large for this system[39;49m"
msgstr "[34m요청된 CPU 셋은 이 시스템에 너무 큽니다[39;49m"

#: fork.c:802
msgid "[35mretrieving CPU affinity set failed[39;49m"
msgstr "[34mCPU affiniy 셋을 검색하는데 실패했습니다[39;49m"
